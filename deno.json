{
    "tasks": {
        "www": "deno run -A cmd/www/main.ts",
        "cache": "deno cache deps.ts"
    },
    "compilerOptions": {
        "jsx": "react-jsx",
        "jsxImportSource": "hono/jsx"
    },
    "imports": {
        "~/deps.ts": "./deps.ts",
        "$hono/": "https://deno.land/x/hono@v3.7.3/",
        "hono/jsx/jsx-runtime": "https://deno.land/x/hono@v3.7.3/jsx/jsx-runtime.ts",
        "$std/": "https://deno.land/std@0.203.0/"
    }
}